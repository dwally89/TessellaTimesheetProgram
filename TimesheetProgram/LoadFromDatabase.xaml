<Window x:Class="TimesheetProgramLogic.LoadDeleteFromDatabase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Load From Database" Height="114" Width="201" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="cboTimesheetID" VerticalAlignment="Top" Width="155" IsReadOnly="True" />
        <Button Content="Load" Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" Name="btnLoad" VerticalAlignment="Top" Width="75" IsDefault="True" Click="BtnLoad_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="93,41,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" IsCancel="True" />
    </Grid>
</Window>
