<Window x:Class="TimesheetProgramLogic.PasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Enter Password" Height="118" Width="265" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Enter password: " Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="115,12,0,0" Name="txtPassword" VerticalAlignment="Top" Width="120" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="79,46,0,0" Name="btnOK" VerticalAlignment="Top" Width="75" IsDefault="True" Click="BtnOK_Click" />
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="160,46,0,0" Name="btnCancel" Width="75" Height="23" VerticalAlignment="Top" IsCancel="True"/>
    </Grid>
</Window>
